<div class="list">
  <h2><%= button_to "x", [list.board, list], method: "delete", remote: true%><%= list.title %></h2>
  <ul id="list-<%=list.id%>" data-id=<%=list.id%> >
    <% list.tasks.each do |task| %>
      <%= render partial: "tasks/task", locals: {task: task} %>
    <% end %>
  </ul>

</div>
